<!doctype html>
<html>
  <head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, minimum-scale=1.0, initial-scale=1.0">
    <meta name="format-detection" content="telephone=no">
    <meta name="author" content="Ryan Cayabyab - ryancayabyab.com">
    <meta name="description" content="">
    
    <!-- Use title if it's in the page YAML frontmatter -->
    <title><%= current_page.data.title || "Cincinnati Design Week 2015 | September 28 - October 2 | Presented by AIGA Cincinnati and AGAR" %></title>

    <%= favicon_tag 'images/favicon.ico' %>
    <%= favicon_tag 'apple-touch-icon-precomposed.png', rel: 'apple-touch-icon-precomposed' %>
    <%= favicon_tag 'apple-touch-icon-57x57-precomposed.png', rel: 'apple-touch-icon-precomposed', size: '57x57' %>
    <%= favicon_tag 'apple-touch-icon-72x72-precomposed.png', rel: 'apple-touch-icon-precomposed', size: '72x72' %>
    <%= favicon_tag 'apple-touch-icon-114x114-precomposed.png', rel: 'apple-touch-icon-precomposed', size: '114x114' %>
    
    <%= stylesheet_link_tag "application" %>
  </head>

  <body class="<%= page_classes %>">
    <%= partial "header" %>
    <div class="bodycontent">
      <%= yield %>
    </div>
    <%= javascript_include_tag "application" %>
  </body>
</html>
